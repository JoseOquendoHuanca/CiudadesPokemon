@model List<ciudades>

    <h1>lista de ciudades pokemon registrados </h1>

@if (Model.Count==0)
{
    <div class="alert alert-warning text-center ">aun no hay ciudades registradas  </div>
}
else{
    <ol>
    @foreach (var p in Model)
    {
       

            <div><img src="@p.foto"/></div>
            <div>@p.nombre</div>
            <div>@p.lider</div>
        
       

    

    }
   
</ol>
}